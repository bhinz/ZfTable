{
    "name": "fagundes/zff-table",
    "description": "Flexible tool to generate table",
    "license": "BSD-3-Clause",
    "keywords": [
        "zftable",
        "table"
    ],
    "authors": [
        {
            "name": "Piotr Duda",
            "email": "dudapiotrek@gmail.com"
        },
        {
            "name": "Vinicius Fagundes",
            "email": "mvlacerda@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "zendframework/zend-mvc":  "^3.0",
        "zendframework/zend-form": "^2.9 | ^3.0",
        "zendframework/zend-view": "^2.8 | ^3.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-0": {
            "ZfTable": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "extra": {
        "zf": {
            "component": "ZfTable",
            "config-provider": "ZfTable\\ConfigProvider"
        }
    }
}
